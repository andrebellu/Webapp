<mvc:View controllerName="sap.ui.demo.walkthrough.controller.Rapportini"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

		<App>
			<Panel
				headerText="{i18n>helloPanelTitle}"
				class="header">
					<ScrollContainer height="100%"
						width="100%"
						vertical="true"
						focusable="true">
				<List noDataText="suca non ci sono elementi" items="{/items}" class="listina">
						<items>
							<StandardListItem type="Navigation" title="{title}" description="{subTitle}" icon="sap-icon://activities" class="item" press=".clicked"/>
						</items>
					<swipeContent>
						<Button press="handleReject" />
					</swipeContent>
				</List>
					</ScrollContainer>
			</Panel>
		</App>

</mvc:View>