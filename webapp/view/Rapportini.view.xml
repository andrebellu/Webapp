<mvc:View controllerName="sap.ui.demo.walkthrough.controller.Rapportini"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">

	<Shell>
		<App>
			<Panel
				headerText="{i18n>helloPanelTitle}"
				class="header">
				<List noDataText="suca non ci sono elementi" items="{/items}" id="list0">
					<items>
						<StandardListItem type="Navigation" title="{title}" description="{subTitle}" icon="sap-icon://activities" id="item0" onPress=".clicked"/>
					</items>
					<swipeContent>
						<Button press="handleReject" />
					</swipeContent>
				</List>
			</Panel>
		</App>
	</Shell>

</mvc:View>